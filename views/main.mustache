<!doctype html>
<html lang="en">
<head>
    <title>My Portfolio</title>
    <meta name="description" content="" />

    {{>head}}
</head>

<body>
    <div class="blocks-page">
        {{>header}}
        
        <section style="background:url('{{{page.hero.img_background}}}=s2048');background-size:cover;background-position:center;">
            <div class="gradient-div py-3" style="height:76vh">
        
                <div class="container">
                    <div class="row justify-content-center header-row">
                        <div class="col-12 col-md-8 col-lg-7 text-center text-white">
                    
                            <img class="mt-2" style="width: 160px;" src="{{{page.hero.img_dev}}}" />
                            
                            <h1 class="display-3 mt-1">{{page.hero.greeting}}</h1>
                            <p class="lead text-white mb-1 mt-0">
                                {{page.hero.intro}}
                                <br/>
                                {{page.hero.intro_2}}
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        
        <!-- Projects Section -->
        <section class="py-3 py-md-7" id="hero_section">
            <div class="container py-md-5">
                <div class="row block-header">
                    <div class="col-sm-8 mx-auto text-center">
                        <h1 class="display-4 mt-1 mb-0">Projects</h1>
                        <hr>
                        <p class="lead mt-0">Some of my works include these projects shown in this gallery.</p>
                        
                    </div>
                </div>
        
                <div class="row mt-5 aos-init aos-animate" data-aos="fade-up" data-aos-duration="1000">
                    {{#projects}}
                    <div class="col-md-4 col-sm-12 mb-2r">
                        <a href="/project/{{slug}}">
                            <div class="position-relative project">
                                <div class="image position-relative ">
                                    <img src="{{{image}}}=s650-c" class="img-fluid w-100 d-block">
                                    <div class="overlay-box">
                                        <div class="overlay-inner overlay-content">
                                            <h5 class="mb-0">{{name}}</h5>
                                            <p>{{category}}</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </a>
                    </div>
                    {{/projects}}
                    
                </div>
                <p class="lead text-center text-dark mt-2" style="font-size: 22px; font-weight: 400;"> To see other small projects and works of mine please head over to my github profile.</p>
                        <div class="text-center"><a class="view-btn text-center" href="https://github.com/LabinotSh/" target="_blank">
                            <button class="btn btn-primary btn-send-message">My github profile <i class="fa fa-github"></i></button></a>
                        </div>
                   
            </div>
        </section>

        <!-- Contact Form -->
        <section id="section3" style="background:url('{{{page.hero.img_3}}}=s2048');background-size:cover;background-position:center;" class="bg-dark">
            <div class="gradient-div py-3">
                <div class="container">
                    <div class="row align-items-center justify-content-between header-row">
                        <div class="col-12 col-md-6 col-lg-6 section-intro">
                            <h1 class="display-4 text-white">{{page.contact.header}}</h1>
                            <span class="lead text-white">
                                {{page.contact.subheader}}
                            </span>
                        </div>
                        <!--end of col-->
                        <div class="col-12 col-md-6 col-lg-5">
                            <div class="card">
                                <div class="card-body">
                                    <div class="text-success text-center" id="success" style="font-size:20px;"></div>
                                    <form action="" method="post">
                                        
                                        <div class="form-group">
                                            <input type="text" id="contact-user-name" class="form-control" placeholder="Name">
                                        </div>
                                        <div class="form-group">
                                            <input type="text" id="contact-user-email" class="form-control" placeholder="Email">
                                        </div>
                                        
                                        <div class="form-group">
                                            <textarea name="" id="contact-user-message" cols="30" rows="7" class="form-control" placeholder="Message"></textarea>
                                        </div>
                                        <div class="form-group text-center">
                                            <input type="submit" id="btn-contact" class="btn btn-primary btn-send-message" value="Send Message">
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--end of row-->
                </div>
            </div>
        </section>
        


        {{>footer}}
    </div>

    {{>scripts}}

    <script src="{{{cdn}}}/js/contact.js"></script>
</body>
</html>
